import java.util.ArrayList;
import java.util.List;

public class Workshop {

	private int clothes;
	private List<Tailor> tailors;
	
	public Workshop(int clothes) {
		this.clothes = clothes;
		tailors = new ArrayList<Tailor>();
	}
	
	public boolean getCloth() {
		if (clothes > 0) {
			--clothes;
			return true;
		} else {
			return false;
		}
	}
	
}
