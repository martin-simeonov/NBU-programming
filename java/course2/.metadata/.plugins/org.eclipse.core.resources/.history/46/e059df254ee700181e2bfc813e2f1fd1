
public class MainClass {

	public static void main(String args[]) {
		Workshop w = new Workshop(10);
		w.addTailor(new Tailor("Tailor 1", 100));
		w.addTailor(new Tailor("Tailor 2", 100));
		w.addTailor(new Tailor("Tailor 3", 100));
		w.startWork();
		w.printWork();
		
		System.out.println("Example with different efficiencies:");
		Workshop w2 = new Workshop(15);
		w2.addTailor(new Tailor("Tailor 1", 20));
		w2.addTailor(new Tailor("Tailor 2", 80));
		w2.addTailor(new Tailor("Tailor 3", 90));
		w2.startWork();
		w2.printWork();
	}
	
}
