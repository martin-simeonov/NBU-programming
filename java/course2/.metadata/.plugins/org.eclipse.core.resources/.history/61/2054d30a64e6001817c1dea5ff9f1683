import java.util.Map;
import java.util.TreeMap;

public class Team {

	private String name;
	private double budget;
	private Coach coach;
	private Map<Player, Double> players;
	private Sport sport;
	
	public Team(String name, double budget, Coach coach, Sport sport) {
		this.name = name;
		this.budget = budget;
		this.coach = coach;
		this.sport = sport;
		this.players = new TreeMap<Player, Double>();
	}
	
}
